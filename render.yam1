services:
  - type: web
    name: jabber
    env: node
    buildCommand: "npm install"
    startCommand: "node jabber.js"
    plan: free
    envVars:
      - key: OPENAI_API_KEY
        sync: false
      - key: GOOGLE_CLIENT_ID
        sync: false
      - key: GOOGLE_CLIENT_SECRET
        sync: false
      - key: OAUTH_REDIRECT
        sync: false
